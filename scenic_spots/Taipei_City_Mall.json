                },
                "publishTime": "2024-09-11T13:16:34.986339Z",
                "flagContentUri": "https://www.google.com/local/review/rap/report?postId=ChdDSUhNMG9nS0VJQ0FnSURIME9tVTRBRRAB&d=17924085&t=1",
                "googleMapsUri": "https://www.google.com/maps/reviews/data=!4m6!14m5!1m4!2m3!1sChdDSUhNMG9nS0VJQ0FnSURIME9tVTRBRRAB!2m1!1s0x3442a96d074fcce9:0xecf052b8f73f292e"
